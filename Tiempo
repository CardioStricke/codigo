package Model;
/**
 * Esta clase contiene y define la info relativa al tiempo (horas , minutos y segundos)
 *@author Bruno Matiacci
 *@version 1.0
 *@param horas hora de la sesión
 *@param minutos minuto de la sesión
 *@param segundos segundo de la sesion
 */

public class Tiempo {
	private int horas;
	private int minutos;
	private int segundos;
	
	
	public Tiempo(int horas, int minutos, int segundos) {
		super();
		this.horas = horas;
		this.minutos = minutos;
		this.segundos = segundos;
	}


	public int getHoras() {
		return horas;
	}


	public void setHoras(int horas) {
		this.horas = horas;
	}


	public int getMinutos() {
		return minutos;
	}


	public void setMinutos(int minutos) {
		this.minutos = minutos;
	}


	public int getSegundos() {
		return segundos;
	}


	public void setSegundos(int segundos) {
		this.segundos = segundos;
	}


	/**
	 * Este método muestra los parámetros
	 *@author Bruno Matiacci
	 *@version 1.0
	 *@return devuelve los valores concretos de los párametros unidos en un string (hora,minuto,segundo)
	 */
	public String toString() {
		return "Tiempo [horas=" + horas + ", minutos=" + minutos + ", segundos=" + segundos + "]";
	}
	
	
}

package Vista;

import java.awt.BorderLayout;
import java.awt.EventQueue;

import javax.swing.JFrame;
import javax.swing.JPanel;
import javax.swing.border.EmptyBorder;
import java.awt.Color;
import javax.swing.JButton;
import javax.swing.JTextField;
import javax.swing.JTextPane;
import javax.swing.JTextArea;
import java.awt.Font;

public class GraficaAltitud extends JFrame {

	private JPanel PanegGraficaAltitud;
	private JTextField textMinimoAltitud;
	private JTextField textMaximoAltitud;

	/**
	 * Launch the application.
	 */
	public static void main(String[] args) {
		EventQueue.invokeLater(new Runnable() {
			public void run() {
				try {
					GraficaAltitud frame = new GraficaAltitud();
					frame.setVisible(true);
				} catch (Exception e) {
					e.printStackTrace();
				}
			}
		});
	}

	/**
	 * Create the frame.
	 */
	public GraficaAltitud() {
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setBounds(100, 100, 734, 487);
		PanegGraficaAltitud = new JPanel();
		PanegGraficaAltitud.setBackground(new Color(224, 255, 255));
		PanegGraficaAltitud.setBorder(new EmptyBorder(5, 5, 5, 5));
		setContentPane(PanegGraficaAltitud);
		PanegGraficaAltitud.setLayout(null);
		
		JButton btnComentariosGraficaAltitud = new JButton("COMENTARIOS");
		btnComentariosGraficaAltitud.setForeground(new Color(128, 0, 0));
		btnComentariosGraficaAltitud.setBackground(Color.WHITE);
		btnComentariosGraficaAltitud.setBounds(552, 396, 156, 42);
		PanegGraficaAltitud.add(btnComentariosGraficaAltitud);
		
		textMinimoAltitud = new JTextField();
		textMinimoAltitud.setBounds(552, 232, 138, 25);
		PanegGraficaAltitud.add(textMinimoAltitud);
		textMinimoAltitud.setColumns(10);
		
		textMaximoAltitud = new JTextField();
		textMaximoAltitud.setBounds(552, 148, 138, 25);
		PanegGraficaAltitud.add(textMaximoAltitud);
		textMaximoAltitud.setColumns(10);
		
		JPanel PGraficaAltitud = new JPanel();
		PGraficaAltitud.setBounds(10, 11, 515, 427);
		PanegGraficaAltitud.add(PGraficaAltitud);
	}
}

package Model;

/**
 * Esta clase contiene los atributos de la cabecera del fichero de la sesion
 *@author Ethan Recalde
 *@version 1.0
 *@param idPaciente numero de identificacion de paciente
 *@param peso peso del paciente
 *@param estado estado de animo del paciente
 *@param comentario "bienvenido"
 */

public class CabeceraSesion {
	
	
	private int idPaciente;
	private float peso;
	private String estado;
	private String comentario;
	public CabeceraSesion(int idPaciente, float peso, String estado, String comentario) {
		super();
		this.idPaciente = idPaciente;
		this.peso = peso;
		this.estado = estado;
		this.comentario = comentario;
	}
	public int getIdPaciente() {
		return idPaciente;
	}
	public void setIdPaciente(int idPaciente) {
		this.idPaciente = idPaciente;
	}
	public float getPeso() {
		return peso;
	}
	public void setPeso(float peso) {
		this.peso = peso;
	}
	public String getEstado() {
		return estado;
	}
	public void setEstado(String estado) {
		this.estado = estado;
	}
	public String getComentario() {
		return comentario;
	}
	public void setComentario(String comentario) {
		this.comentario = comentario;
	}
	
	/**
	 * Este metodo muestra los parametros
	 *@author Ethan Recalde
	 *@version 1.0
	 *@return devuelve los valores de los parametros unidos en un string
	 */
	public String toString() {
		return "CabeceraSesion [idPaciente=" + idPaciente + ", peso=" + peso + ", estado=" + estado + ", comentario="
				+ comentario + "]";
	}
}

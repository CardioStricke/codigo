		PaneGraficaPulsaciones.add(btnComentariosGraficaPulsaciones);
		
		textRiesgoPulsaciones = new JTextField();
		textRiesgoPulsaciones.setBounds(560, 210, 86, 20);
		PaneGraficaPulsaciones.add(textRiesgoPulsaciones);
		textRiesgoPulsaciones.setColumns(10);
		
		textMinimoPulsaciones = new JTextField();
		textMinimoPulsaciones.setBounds(560, 153, 86, 20);

		textMinimoPulsaciones.setColumns(10);
		
		textMaximoPulsaciones = new JTextField();
		textMaximoPulsaciones.setBounds(560, 92, 86, 20);
		PaneGraficaPulsaciones.add(textMaximoPulsaciones);
		textMaximoPulsaciones.setColumns(10);
		
		JPanel PGraficaPulsaciones = new JPanel();
		PGraficaPulsaciones.setBounds(10, 11, 520, 426);

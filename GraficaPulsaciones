public class GraficaPulsaciones extends JFrame {

	private JPanel PaneGraficaPulsaciones;
	private JTextField textRiesgoPulsaciones;
	private JTextField textMinimoPulsaciones;
	private JTextField textMaximoPulsaciones;

	/**
	 * Launch the application.
	 */
	public static void main(String[] args) {
		EventQueue.invokeLater(new Runnable() {

				try {
					GraficaPulsaciones frame = new GraficaPulsaciones();
					frame.setVisible(true);
				} catch (Exception e) {
					e.printStackTrace();
				}
			}
		});
	}

	/**
	 * Create the frame.
	 */
	public GraficaPulsaciones() {
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setBounds(100, 100, 735, 486);
		PaneGraficaPulsaciones = new JPanel();
		PaneGraficaPulsaciones.setBackground(new Color(224, 255, 255));
		PaneGraficaPulsaciones.setBorder(null);
		setContentPane(PaneGraficaPulsaciones);
		PaneGraficaPulsaciones.setLayout(null);
		
		JButton btnComentariosGraficaPulsaciones = new JButton("COMENTARIOS");
		btnComentariosGraficaPulsaciones.setForeground(new Color(128, 0, 0));
		btnComentariosGraficaPulsaciones.setBackground(Color.WHITE);
